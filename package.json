{
  "name": "webpage-builder",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "start": "turbo start --filter=nx-wrapper",
    "dev": "turbo dev --filter=nx-wrapper",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
